VITE_APPSYNC_API_KEY=YOUR_DEV_APPSYNC_API_KEY
VITE_API_BASE_URL=
VITE_API_GATEWAY_KEY=

# Cognito Hosted UI (set these to enable social sign-in)
# Example values:
# VITE_COGNITO_DOMAIN=your-domain.auth.us-east-2.amazoncognito.com
# VITE_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
# VITE_COGNITO_REDIRECT_SIGNIN=http://localhost:8081/dashboard
# Optional scopes (space separated or + encoded)
# VITE_COGNITO_SCOPE=openid+email+profile

# Feature flags
# Set to 'true' to re-enable email confirmation workflow in LocalSignUp
VITE_ENABLE_EMAIL_CONFIRMATION=false
